{
  "quest_log_regions": {
    "main_quest_area": [100, 200, 800, 600],
    "quest_status_area": [900, 200, 300, 600],
    "chain_progress_area": [100, 800, 1100, 200],
    "quest_log_window": [50, 150, 1200, 700],
    "completion_status_area": [1000, 300, 250, 400]
  },
  "completion_keywords": [
    "completed",
    "finished", 
    "done",
    "accomplished",
    "fulfilled",
    "quest complete",
    "mission complete",
    "objective complete",
    "reward collected",
    "quest finished",
    "chain complete",
    "successfully completed",
    "quest accomplished",
    "mission finished",
    "objective fulfilled"
  ],
  "chain_identifiers": [
    "legacy",
    "theme park",
    "heroic",
    "epic",
    "daily",
    "weekly",
    "faction",
    "profession",
    "crafting",
    "combat",
    "social",
    "pilot",
    "entertainer",
    "medic",
    "commando",
    "officer",
    "smuggler",
    "spy",
    "trader",
    "artisan"
  ],
  "confidence_thresholds": {
    "min_confidence": 70.0,
    "high_confidence": 85.0,
    "very_high_confidence": 95.0
  },
  "retry_settings": {
    "retry_attempts": 3,
    "retry_delay": 1.0,
    "max_verification_time": 10.0
  },
  "cache_settings": {
    "cache_duration": 300,
    "max_cache_size": 100,
    "enable_cache": true
  },
  "user_prompts": {
    "enable_prompts": true,
    "prompt_on_completed_quest": true,
    "prompt_on_completed_chain": true,
    "auto_skip_high_confidence": false
  },
  "ocr_settings": {
    "methods": ["standard", "aggressive", "conservative"],
    "preprocessing": {
      "enable_noise_reduction": true,
      "enable_contrast_enhancement": true,
      "enable_text_cleaning": true
    },
    "text_validation": {
      "min_text_length": 10,
      "max_text_length": 1000,
      "enable_fuzzy_matching": true
    }
  },
  "quest_patterns": {
    "completion_patterns": [
      "{quest_name}.*completed",
      "{quest_name}.*finished", 
      "{quest_name}.*done",
      "completed.*{quest_name}",
      "finished.*{quest_name}",
      "quest.*{quest_name}.*complete",
      "mission.*{quest_name}.*complete"
    ],
    "proximity_threshold": 200,
    "case_sensitive": false
  },
  "logging": {
    "log_level": "INFO",
    "log_verification_results": true,
    "log_cache_hits": true,
    "log_ocr_errors": true,
    "log_user_prompts": true
  },
  "integration": {
    "auto_integrate_with_quest_system": true,
    "hook_into_quest_execution": true,
    "prevent_redundant_quests": true,
    "enable_chain_optimization": true
  }
} 